<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<!--frame use-->
	<package name="frameAction" namespace="/" extends="abstract_struts">
		<action name="frameAction_*" class="com.inspur.cmis.action.FrameAction" method="{1}">
			<result name="left">/WEB-INF/page/frame/left.jsp</result>
			<result name="index">/WEB-INF/page/frame/index.jsp</result>
			<result name="top">/WEB-INF/page/frame/top.jsp</result>
		</action>
	</package>
</struts>