<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.inspur.cmis.entity.PmParamEntity" table="PM_PARAM" schema="C##SUNJIE">
        <id name="id" column="ID"/>
        <property name="parentid" column="PARENTID"/>
        <property name="classid" column="CLASSID"/>
        <property name="code" column="CODE"/>
        <property name="name" column="NAME"/>
        <property name="valid" column="VALID"/>
        <property name="description" column="DESCRIPTION"/>
        <property name="updatedate" column="UPDATEDATE"/>
        <property name="userid" column="USERID"/>
    </class>
</hibernate-mapping>